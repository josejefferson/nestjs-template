{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "NestJS template",
  "author": {
    "name": "Jefferson Dantas",
    "url": "https://github.com/josejefferson"
  },
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "NODE_ENV=production node dist/main",
    "dev": "nest start --watch",
    "build": "nest build",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "tsc --noEmit",
    "migrate:generate": "dotenv -- typeorm-ts-node-commonjs migration:generate src/migrations/migration -d src/config/orm.config.ts",
    "migrate:run": "dotenv -- typeorm-ts-node-commonjs migration:run -d src/config/orm.config.ts"
  },
  "dependencies": {
    "@dataui/crud": "^5.3.1",
    "@dataui/crud-typeorm": "^5.3.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/typeorm": "^10.0.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.32.3",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  }
}
